# Unix
alias ll="ls -la"
alias ln="ln -v"
alias mkdir="mkdir -p"
alias e="$EDITOR"
alias v="$VISUAL"
alias vim="nvim"

# Pretty print the path
alias path='echo $PATH | tr -s ":" "\n"'

alias vimdirt="nvim `git status --porcelain | sed -ne 's/^ M //p'`"
alias python="python3"
alias python2="python2"
