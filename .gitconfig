[push]
  default = current
[color]
  ui = auto
[alias]
  aa = add --all
  ap = add --patch
  branches = for-each-ref --sort=-committerdate --format=\"%(color:blue)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\" refs/remotes
  ci = commit -v
  co = checkout
  st = status
[merge]
  ff = only
[fetch]
  prune = true
[rebase]
  autosquash = true
[credential]
	helper = osxkeychain
[user]
  name = Zander Mackie
  email = zander@skilledup.com
[code]
  editor = /usr/local/bin/vim
[core]
	editor = /usr/local/bin/vim
